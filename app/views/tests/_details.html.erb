<div class="box box-default">
  <div class="box-body">
        <table class="table">
          <tbody>
          <tr>
            <td><b>Name</b></td>
            <td><%= @test.name %></td>
          </tr>
          <tr>
            <td><b>Result</b></td>
            <%
              begin
              result = Result.find(@test.result_id)
            %>
            <td><button type="button" class="btn btn-block <%= result.display_style_btn %> btn-xs"><%= result.name %></button></td>
            <% end %>
          </tr>
          <tr>
            <td><b>Started</b></td>
            <td><%= @test.start_time.to_s(:db) %></td>
          </tr>
          <tr>
            <td><b>Duration</b></td>
            <td><%= @test.display_duration %></td>
          </tr>
          </tbody>
        </table>
  </div>
</div>

<div class="box box-default">
  <div class="box-body">
    <%= @test.log %>
  </div>
</div>